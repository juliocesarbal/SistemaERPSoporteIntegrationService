              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consume datos de SQL Server, OLE DB, ODBC u Oracle mediante el correspondiente proveedor de datos de .NET Framework. Se usa una instrucción Transact-SQL para definir el conjunto de resultados. Por ejemplo, se extraen datos de SQL Server con el proveedor de datos de .NET Framework para SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; Todos los derechos reservados; http://www.microsoft.com/sql/support;4"
              description="Consume datos de SQL Server, OLE DB, ODBC u Oracle mediante el correspondiente proveedor de datos de .NET Framework. Se usa una instrucción Transact-SQL para definir el conjunto de resultados. Por ejemplo, se extraen datos de SQL Server con el proveedor de datos de .NET Framework para SQL Server."
              name="Origen de ADO NET"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    fs.codigo AS idCompra,
    dt.tiempo_key AS idTiempo,
    dun_dw.codigo AS idUnidad_Negocio,
    dr.codigo AS idRegional,
    dp.idProveedor,
    dpd.idProducto,

    fs.sub_total_con_descuento,
    fs.total,
    dfs.pendiente,
    dfs.cantidad,
    (fs.total - dfs.pendiente) AS total_actual,

    -- Total anterior del proveedor (última compra)
    ISNULL((
        SELECT TOP 1 anterior.total
        FROM dberp.factura_suplidor anterior
        WHERE anterior.suplidor = fs.suplidor
          AND anterior.fecha &lt; fs.fecha
        ORDER BY anterior.fecha DESC
    ), 0) AS total_anterior,

    -- Días entre compras del mismo proveedor
    ISNULL((
        SELECT TOP 1 DATEDIFF(DAY, anterior.fecha, fs.fecha)
        FROM dberp.factura_suplidor anterior
        WHERE anterior.suplidor = fs.suplidor
          AND anterior.fecha &lt; fs.fecha
        ORDER BY anterior.fecha DESC
    ), 0) AS dias_between_pedidos,

    -- Cantidad de compras anteriores al proveedor
    (
        SELECT COUNT(*)
        FROM dberp.factura_suplidor anterior
        WHERE anterior.suplidor = fs.suplidor
          AND anterior.fecha &lt; fs.fecha
    ) AS cantidad_intervalos_pedido

FROM dberp.factura_suplidor fs
JOIN dberp.detalle_factura_suplidor dfs ON fs.codigo = dfs.factura_suplidor
JOIN dberp.articulo a ON dfs.articulo = a.codigo
JOIN dberp.unidad_de_negocio dun ON fs.unidad_de_negocio = dun.codigo
JOIN dberp.suplidor s ON fs.suplidor = s.codigo

-- Dimensiones DW
LEFT JOIN SoporteerpDW.dbo.dimTiempo dt ON CAST(fs.fecha AS DATE) = dt.Fecha
LEFT JOIN SoporteerpDW.dbo.dimUnidad_Negocio dun_dw ON dun.codigo = dun_dw.codigo
LEFT JOIN SoporteerpDW.dbo.dimRegional dr ON dun.codigo = dr.codigo
LEFT JOIN SoporteerpDW.dbo.dimProveedor dp ON s.codigo = dp.idProveedor
LEFT JOIN SoporteerpDW.dbo.dimProducto dpd ON a.codigo = dpd.idProducto

-- Evitar duplicados
WHERE NOT EXISTS (
    SELECT 1
    FROM SoporteerpDW.dbo.Hecho_Compras hc
    WHERE hc.idCompra = fs.codigo
      AND hc.idProducto = dpd.idProducto
);</property>
