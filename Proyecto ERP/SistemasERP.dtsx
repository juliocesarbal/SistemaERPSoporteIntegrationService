              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT
    dt.tiempo_key,
    ric.cliente AS idCliente,
    ric.unidad_de_negocio AS codigo_unidad_negocio,
    dric.tipo_documento AS codigo_tipo_documento,
    ric.monto AS monto_reconciliacion,
    SUM(dric.credito) AS monto_credito,
    SUM(dric.debito) AS monto_debito,
    COUNT(dric.codigo) AS cantidad_documentos,
    ric.anulada,
    ric.fecha_anulada,
    ric.razon_anulacion,
    ric.imprimir
FROM Soporteerp.dberp.reconciliacion_interna_cliente ric
INNER JOIN Soporteerp.dberp.detalle_reconciliacion_interna_cliente dric
    ON dric.reconciliacion = ric.codigo
INNER JOIN Soporteerp.dberp.cliente c
    ON c.codigo = ric.cliente
INNER JOIN Soporteerp.dberp.unidad_de_negocio un
    ON un.codigo = ric.unidad_de_negocio
INNER JOIN Soporteerp.dberp.tipo_documento td
    ON td.codigo = dric.tipo_documento
LEFT JOIN SoporteerpDW.dbo.dimTiempo dt
    ON CAST(ric.fecha AS DATE) = dt.Fecha
LEFT JOIN SoporteerpDW.dbo.Hecho_Reconciliacion_Interna h
    ON h.tiempo_key = dt.tiempo_key
    AND h.idCliente = ric.cliente
    AND h.codigo_unidad_negocio = ric.unidad_de_negocio
    AND h.codigo_tipo_documento = dric.tipo_documento
    AND h.monto_reconciliacion = ric.monto
WHERE h.tiempo_key IS NULL
GROUP BY
    dt.tiempo_key,
    ric.cliente,
    ric.unidad_de_negocio,
    dric.tipo_documento,
    ric.monto,
    ric.anulada,
    ric.fecha_anulada,
    ric.razon_anulacion,
    ric.imprimir
;</property>
