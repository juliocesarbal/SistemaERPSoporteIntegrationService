    WHERE f.fecha IS NOT NULL

    UNION

    SELECT 
        CAST(fs.fecha AS DATE) AS Fecha,
        YEAR(fs.fecha) AS Año,
        MONTH(fs.fecha) AS Mes,
        DATEPART(QUARTER, fs.fecha) AS Trimestre,
        DAY(fs.fecha) AS Dia
    FROM dberp.factura_suplidor fs
    WHERE fs.fecha IS NOT NULL

